<h1> Create a Song: </h1>
<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :song_artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :song_genre_name %>
  <%= f.collection_select :song_genre_id, Genre.all, :name, :name %>

  <%= f.submit %>
<% end %>
