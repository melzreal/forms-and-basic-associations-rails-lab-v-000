<h1> Create a Song: </h1>
<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :song_artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :song_genre_name %>
  <%= f.collection_select :genre_id, Genre.all, :name, :name %>
  <%= f.text_field :song_notes_1 %>
  <%= f.text_field :song_notes_2 %>
  <br>
  <input name="song_notes_1[song_ids][]">
  <br>
  <input name="song_notes_2[song_ids][]">
  <%= f.submit %>
<% end %>
